{
  "name": "superphp",
  "description": "Super PHP Framework.",
  "keywords": ["framework", "sp"],
  "license": "MIT",
  "homepage": "https://superphp.com",
  "support": {
  },
  "authors": [
    {
      "name": "Phil",
      "email": "Phil@26no.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "doctrine/inflector": "~1.1",
    "erusev/parsedown": "~1.6",
    "league/flysystem": "~1.0",
    "monolog/monolog": "~1.11",
    "mtdowling/cron-expression": "~1.0",
    "nesbot/carbon": "~1.20",
    "paragonie/random_compat": "~1.4|~2.0",
    "ramsey/uuid": "~3.0",
    "swiftmailer/swiftmailer": "~5.4",
    "symfony/console": "~3.2",
    "symfony/debug": "~3.2",
    "symfony/finder": "~3.2",
    "symfony/http-foundation": "~3.2",
    "symfony/http-kernel": "~3.2",
    "symfony/process": "~3.2",
    "symfony/routing": "~3.2",
    "symfony/var-dumper": "~3.2",
    "tijsverkoyen/css-to-inline-styles": "~2.2",
    "vlucas/phpdotenv": "~2.2"
  },
  "require-dev": {
    "predis/predis": "^1.1.1",
    "doctrine/dbal": "~2.5",
    "mockery/mockery": "~0.9.4",
    "pda/pheanstalk": "~3.0",
    "phpunit/phpunit": "~5.7",
    "symfony/css-selector": "~3.2",
    "symfony/dom-crawler": "~3.2"
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "Super\\": "src//Super"
    }
  },
  "autoload-dev": {
    "files": [
    ],
    "psr-4": {
      "Super\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "suggest": {
    "predis/predis": "Required to use the redis cache and queue drivers (~1.0)."
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
